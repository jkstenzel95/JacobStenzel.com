  src url("fonts/Trajan Pro Regular.ttf")

// ----------------USEFUL CONSTANTS----------------

// Extra small screen / phone
$screen-xs = 480px
$screen-xs-max = 767px
// Small screen / tablet
$screen-sm = 768px
$screen-sm-max = 991px
// Medium screen / desktop
$screen-md = 992px
$screen-md-max = 1199px
// Large screen / wide desktop
$screen-lg = 1200px

$logo-font = "Trajan Pro Regular"
//$logo-font = "Josefin Sans"

$general-font = "Questrial"
//$general-font = "Open Sans"

$general-line-height = 1.25

$general-font-size = 15px

$transform-properties = "-webkit-transform, -moz-transform, -ms-transform, -o-transform, transform"

// ----------------END USEFUL CONSTANTS----------------


// Font fix
body {
  -webkit-animation-duration: 0.1s;
  -webkit-animation-name: fontfix;
  -webkit-animation-iteration-count: 1;
  -webkit-animation-timing-function: linear;
  -webkit-animation-delay: 0.1s;
}

@-webkit-keyframes fontfix {
  from { opacity: 1; }
  to { opacity: 1; }
}

// ----------------MIXINS----------------


vendor-blur(blurAmt)
  -webkit-filter blur(blurAmt)
  -moz-filter blur(blurAmt)
  -o-filter blur(blurAmt)
  -ms-filter blur(blurAmt)
  filter blur(blurAmt)
    
vendor-transform(type)
  -webkit-transform: unquote(type);
  -moz-transform: unquote(type);
  -ms-transform: unquote(type);
  -o-transform: unquote(type);
  transform: unquote(type);
  
vendor-transition-property(properties)
  -webkit-transition-property unquote(properties)
  -moz-transition-property unquote(properties)
  -o-transition-property unquote(properties)
  transition-property unquote(properties)

vendor-transition-duration(duration)
  -webkit-transition-duration duration
  -moz-transition-duration duration
  -o-transition-duration duration
  transition-duration duration

vendor-transition-timing-function(t-function)
  -webkit-transition-timing t-function
  -moz-transition-timing t-function
  -o-transition-timing t-function
  transition-timing t-function

vendor-transition-delay(delay)
  -webkit-transition-delay delay;
  -moz-transition-delay delay;
  -o-transition-delay delay;
  transition-delay delay;
  

// ----------------END MIXINS----------------


