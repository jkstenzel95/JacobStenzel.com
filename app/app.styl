    vendor-transform("scaleY(1)");
    max-height 180px

  &.ng-leave
    display block !important
    vendor-transition-delay(0s)
    vendor-transition-property($transform-properties+", max-height")
    vendor-transition-timing-function(ease-in-out)
    vendor-transition-duration(0.4s)
    vendor-transform("scaleY(1)");
    max-height 180px
    
  &.ng-leave-active
    vendor-transform("scaleY(0)");
    position relative
    max-height 0px

.blog-preview-wrapper
  display: block; /* Fallback for non-webkit */
  display: -webkit-box;
  height: $general-font-size*$general-line-height*$blog-lines-to-show; /* Fallback for non-webkit */
  font-size: $font-size;
  line-height: $line-height;
  -webkit-line-clamp: $blog-lines-to-show;
  -webkit-box-orient: vertical;
  overflow: hidden;
  text-overflow: ellipsis;
  
.blog-read-more
  cursor pointer
  &:hover
    text-decoration underline
    
.blog-right-wrapper
  width 100%
  
      
// End blog styling

// Begin login styling

.login-rack
  position fixed
  right 0
  z-index 21
  padding-right 10px
  line-height 60px
  font-size 30px
  .glyphicon
    padding-left 10px
  a
    color #83a6ec
    text-decoration none
    &:hover
      color #A4BEF1
      text-decoration underline
    &:active
      text-decoration underline
      
.login
  position absolute
  top 0%
  text-align center
  width 100%
  background-color #83a6ec
  box-shadow 1px 1px 1px 1px #608de6 inset
  h1
    margin-top 0px
    text-align center
  .input-group
    width 100%
    margin 5px
  .btn-group
    margin 5px

    
  &.ng-hide-remove
    display block !important
    vendor-transition-delay(0s)
    vendor-transition-property("top")
    vendor-transition-timing-function(ease-in-out)
    vendor-transition-duration(0.4s)
    top -200%
    
  &.ng-hide-remove-active
    top 0%

  &.ng-hide-add
    display block !important
    vendor-transition-delay(0s)
    vendor-transition-property("top")
    vendor-transition-timing-function(ease-in-out)
    vendor-transition-duration(0.4s)
    top 0%
    
  &.ng-hide-add-active
    top -200%
    
.login-placeholder
  @extend .login
  position relative
  opacity 0
  z-index 0
  
  &.ng-hide-remove
    vendor-transition-property("none")
    top 0%
    
  &.ng-hide-remove-active
    top 0%

  &.ng-hide-add
    vendor-transition-property("none")
    top 0%
    
  &.ng-hide-add-active
    top 0%

// End login styling

// Begin admin styling

.admin-landing
  text-align center
  margin-top 10px
  margin-bottom 10px
  vendor-transition-delay(0s)
  vendor-transition-property("transform")
  vendor-transition-timing-function(ease-out)
  vendor-transition-duration(0.2s)
  transform scale(1)
  &:hover
    transform scale(1.1)
  h1
    margin-top 0
    margin-bottom 0

// End admin styling



